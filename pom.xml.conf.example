<rule>
	<target type="tcpkill"><param name="severity">3</param></target>
	<matches>
		<match type="ethernet"/>
		<match type="ipv4">
			<param name="daddr">1.2.3.4</param>
			<param name="dnetmask">255.255.255.255</param>
		</match>
		<match type="tcp">
			<param name="dport">80</param>
		</match>
	</matches>
</rule>
</config>
